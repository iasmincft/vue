ink.vue';\n\nexport default {\n    components: {\n        NavbarLink\n    },\n    created() {\n        this.getThemeSetting();\n    },\n    computed: {\n\n    },\n    props: ['pages', 'activePage', 'navLinkClick'],\n    data(){\n        return {\n            theme: 'light'\n        }\n    },\n    methods: {\n        changeTheme() {\n            let theme = 'light';\n\n            if (this.theme == 'light') {\n                theme = 'dark';\n            }\n            \n            this.theme = theme;\n            this.storeThemeSetting();\n        },\n        storeThemeSetting() {\n          localStorage.setItem('theme', this.theme);\n        },\n        getThemeSetting() {\n            let theme = localStorage.getItem('theme');\n\n            if(theme) {\n                this.theme = theme;\n            }\n        } \n    }\n}\n\n</script>"],"mappings":"AA0BA,OAAOA,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EACDE,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,eAAe,CAAC,CAAC;EAC1B,CAAC;EACDC,QAAQ,EAAE,CAEV,CAAC;EACDC,KAAK,EAAE,CAAC,OAAO,EAAE,YAAY,EAAE,cAAc,CAAC;EAC9CC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,KAAK,EAAE;IACX;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAG;MACV,IAAIF,KAAI,GAAI,OAAO;MAEnB,IAAI,IAAI,CAACA,KAAI,IAAK,OAAO,EAAE;QACvBA,KAAI,GAAI,MAAM;MAClB;MAEA,IAAI,CAACA,KAAI,GAAIA,KAAK;MAClB,IAAI,CAACG,iBAAiB,CAAC,CAAC;IAC5B,CAAC;IACDA,iBAAiBA,CAAA,EAAG;MAClBC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAE,IAAI,CAACL,KAAK,CAAC;IAC3C,CAAC;IACDJ,eAAeA,CAAA,EAAG;MACd,IAAII,KAAI,GAAII,YAAY,CAACE,OAAO,CAAC,OAAO,CAAC;MAEzC,IAAGN,KAAK,EAAE;QACN,IAAI,CAACA,KAAI,GAAIA,KAAK;MACtB;IACJ;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}